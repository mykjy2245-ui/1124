#include <iostream>
#include <vector>
using namespace std;

// 삽입 정렬: 앞 부분은 정렬되어 있다고 가정하고 새 값을 알맞은 위치에 삽입하는 방식
void insertionSort(vector<int>& arr) {
    int n = arr.size();

    for (int i = 1; i < n; i++) {
        int key = arr[i];    // 삽입할 값
        int j = i - 1;

        // key보다 큰 값들은 한 칸씩 뒤로 이동
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j--;
        }

        // key를 제자리(j+1)에 삽입
        arr[j + 1] = key;
    }
}

int main() {
    vector<int> arr = {12, 11, 13, 5, 6};
    insertionSort(arr);

    for (int x : arr) cout << x << " ";
    return 0;
}
